{
  "app.disableAutoupdates": false,
  "app.hideMenubarIcon": false,
  "app.launchOnStartup": true,
  "autocomplete.disable": true,
  "autocomplete.theme": "dracula",
  "chat.defaultModel": "claude-sonnet-4.6",
  "chat.enableCheckpoint": true,
  "chat.enableCodeIntelligence": true,
  "chat.enableKnowledge": true,
  "chat.enableNotifications": true,
  "chat.enableSubagent": true,
  "chat.enableTangentMode": true,
  "chat.enableThinking": false,
  "chat.enableTodoList": true,
  "codeWhisperer.shareCodeWhispererContentWithAWS": false,
  "knowledge.defaultExcludePatterns": [
    "**/target/**",
    "**/build/**",
    "**/.git/**",
    "**/.idea/**",
    "**/.metadata/**",
    "**/.vscode/**",
    "**/.classpath/**",
    "**/.project/**",
    "**/node_modules/**",
    "**/*.class",
    "**/*.jar",
    "**/*.iml",
    "**/*.flattened-pom.xml",
    "**/.DS_Store"
  ],
  "knowledge.maxFiles": 3000,
  "mcp.loadedBefore": true,
  "telemetry.enabled": false,
  "chat.defaultAgent": "petru_agent"
}